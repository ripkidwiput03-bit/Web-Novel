# Fancy directory listing\
lf() {\
    echo -e "\033[1;36mÉøÉ≥Ç Contents of: \033[1;33m$(pwd)\033[0m"\
    echo -e "\033[2;36m$(printf '‚É¥Ä%.0s' {1..50})\033[0m"\
    ls -la --color=auto "$@"\
    echo -e "\033[2;36m$(printf '‚É¥Ä%.0s' {1..50})\033[0m"\
    local count=$(ls -1 "$@" 2>/dev/null | wc -l)\
    echo -e "\033[2m  ÉøÉ≥É™ Total: ${count} items\033[0m"\
}\
\
# Extract any archive\
ex() {\
    if [ -f "$1" ]; then\
        case "$1" in\
            *.tar.bz2) tar xjf "$1" ;;\
            *.tar.gz)  tar xzf "$1" ;;\
            *.bz2)     bunzip2 "$1" ;;\
            *.rar)     unrar x "$1" ;;\
            *.gz)      gunzip "$1" ;;\
            *.tar)     tar xf "$1" ;;\
            *.tbz2)    tar xjf "$1" ;;\
            *.tgz)     tar xzf "$1" ;;\
            *.zip)     unzip "$1" ;;\
            *.Z)       uncompress "$1" ;;\
            *.7z)      7z x "$1" ;;\
            *) echo "'$1' cannot be extracted" ;;\
        esac\
    else\
        echo "'$1' is not a valid file"\
    fi\
}\
