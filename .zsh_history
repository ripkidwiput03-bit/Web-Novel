# Color palette display\
colors() {\
    echo ""\
    echo -e "  \033[1;37m‚ÉµÉ∞‚ÉµÉ∞‚ÉµÉ∞ COLOR PALETTE ‚ÉµÉ∞‚ÉµÉ∞‚ÉµÉ∞\033[0m"\
    echo ""\
    for i in {0..255}; do\
        printf "\033[48;5;%sm  %3s  \033[0m" "$i" "$i"\
        if [ $(( (i + 1) % 8 )) -eq 0 ]; then\
            echo ""\
        fi\
    done\
    echo ""\
}\
