# ‚É¥Ä‚É¥Ä‚É¥Ä PULSE ANIMATION ‚É¥Ä‚É¥Ä‚É¥Ä\
pulse_text() {\
    local text="$1"\
    local colors=($R $Y $G $C $B $M)\
    \
    for ((i=0; i<3; i++)); do\
        for color in "${colors[@]}"; do\
            printf "\r  ${color}‚¨§ ${text}${N}  "\
            sleep 0.08\
        done\
    done\
    printf "\r  ${G}‚¨§ ${text}${N}  \n"\
}\
