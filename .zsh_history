# Create project directory\
mkproject() {\
    if [ -z "$1" ]; then\
        echo "Usage: mkproject <project-name>"\
        return 1\
    fi\
    mkdir -p "$1"/{src,docs,tests,assets}\
    touch "$1"/README.md\
    touch "$1"/src/main.py\
    echo "# $1" > "$1"/README.md\
    echo -e "\033[1;32m‚ÉºÉ≥ Project '$1' created with structure:\033[0m"\
    find "$1" -type f -o -type d | head -20 | sed 's/^/  /'\
    cd "$1"\
}\
